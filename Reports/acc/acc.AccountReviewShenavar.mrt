      </AccountReviewDataDS>
      <ParametersDS Ref="4" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>ParametersDS</Alias>
        <Columns isList="true" count="5">
          <value>FinancialDocType_Text,System.String</value>
          <value>NoeTaraz_Text,System.String</value>
          <value>FinancialYear_Text,System.String</value>
          <value>OrganizationName,System.String</value>
          <value>RemainedStatus_Text,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>ParametersDS</Name>
        <NameInSource>MainDBConnection</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>select
	(select Title from afw_OptionSetItems where ID = {FinancialDocType}) FinancialDocType_Text,
(select YearNo from cmn_FinancialYears where ID = {FinancialYear}) FinancialYear_Text,
(select Name from cmn_OrganizationInformations where ID = {OrganizationUnitID}) OrganizationName,
	case 
		when {NoeTaraz} = 'AllCoding' then N'همه کدینگ'
		when {NoeTaraz} = 'BalanceSheetCoding' then N'کدینگ تراز نامه'
		when {NoeTaraz} = 'CostAndBenefitCoding' then N'کدینگ سود و زیان'
	end NoeTaraz_Text,
	case 
		when {RemainedStatusFilter} = 'All' then N'همه '
		when {RemainedStatusFilter} = 'UnZeroRemained' then N'مانده غیر صفر'
		when {RemainedStatusFilter} = 'RemainingDebtorAmount' then N'مانده بدهکار'
		when {RemainedStatusFilter} = 'RemainingCreditorAmount' then N'مانده بستانکار'
		else ' - '
	end RemainedStatus_Text</SqlCommand>
      </ParametersDS>
