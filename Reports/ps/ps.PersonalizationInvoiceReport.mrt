        <Parameters isList="true" count="0" />
        <SqlCommand>
select saleInvc.InvoiceNumber,
	Person.FullName,
	afw.GregorianToPersian(saleInvc.ValidityDate) ValidityDate,
	saleInvc.ValidityDays,
	saleInvc.DeliverDays,
	afw.GregorianToPersian(saleInvc.IssueDate) IssueDate,
	afw.GregorianToPersian(saleInvc.DeliverDate) DeliverDate, 
	isnull(saleInvc.MohlatePardakht, '') MohlatPardakht, 
	saleInvc.Description, 
	isnull(CustomPay.Title, '') +' '+ isnull(saleInvc.CustompayMethodDesc,'')  CustomPay, 
	isnull(InstallMthd.Title, '') +' '+ isnull(saleInvc.InstallMethodDesc,'')  InstallMthd, 
	isnull(DelyMthd.Title, '') +' '+ isnull(saleInvc.DeliverMethodDesc,'')  DelyMthdTitle,
	isnull(ConPersonGender.Title, '') + ' ' + isnull(ConPerSon.Name,'') + ' ' + isnull(ConPerSon.LastName,'')  LastNameCon,
	InvcItm.RowNumber,
	case when InvcItm.Stuff is null
		then service.Name
		else isnull(stuffs.CustomName,StuffDefs.Name) 
	end StuffOrServiceName,
	InvcItm.UnitPrice,
	InvcItm.TotalPrice,
	InvcItm.Discount,
	Units.Title  UnitTitle,  
	InvcItm.TotalPriceAfterDiscount, 
	saleInvc.TotalTaxAndToll, 
	saleInvc.TotalDiscount,
	case
		when States.StatePreNumber is null 
			then case 
				when Person.MobilePhoneNumber1 is null then Person.TelNumber1
				else Person.TelNumber1 +N' و ' +Person.MobilePhoneNumber1
			end
		else case 
				when Person.MobilePhoneNumber1 is null then States.StatePreNumber + Person.TelNumber1
				else States.StatePreNumber + Person.TelNumber1 +N' و ' +Person.MobilePhoneNumber1
			end
		end TelNumber1,
	coalesce(States.StatePreNumber + Person.TelNumber1, '') + N' و ' + Person.MobilePhoneNumber1 TelNumber,
	coalesce(States.StatePreNumber + Person.Fax, '') Fax,
	Person.Address1, 
	Person.PostalCode,
	InvcItm.Quantity,
	InvcItm.TedadMahGaranti,
	saleInvc.FinalAmount,
	StuffDefs.Notes as Notes,
	case when EkhtelafPriceList.EkhtelafPrice = 0 then '' else cast(cast(EkhtelafPriceList.EkhtelafPrice as bigint) as varchar) end EkhtelafPrice,	
	case when InvcItm.TotalPrice != 0
		then case round((cast(InvcItm.Discount as float)/CAST( InvcItm.TotalPrice as float)) * 100, 2) 
				when 0 then '-' 
				else cast(round((cast(InvcItm.Discount as float)/CAST( InvcItm.TotalPrice as float)) * 100, 2 )as varchar(10)) 
		end
		else '-'
	end DiscountPerecent,
	Cofing.Proforma_MatneMolahezatPishFactor as MatneMolahezatPishFactor,
	isnull(States.Name, '') + coalesce(' - ' + Cities.Name, '') StateAndCityName 
from ps_SalesInvoices saleInvc 
	inner join ps_SalesInvoiceItems InvcItm on saleInvc.ID = InvcItm.SalesInvoice 
	left join cmn_Stuffs stuffs on InvcItm.Stuff = stuffs.ID 
	inner join cmn_PersonsView Person on saleInvc.Customer = Person.ID 	
	left join afw_OptionSetItems PersonGender on PersonGender.ID = Person.Gender
	left join ps_InvoiceCustomPayMethods CustomPay on saleInvc.CustompayMethod = CustomPay.ID 
	left join ps_InvoiceInstallMethods InstallMthd on saleInvc.InstallMethod = InstallMthd.ID 
	left join ps_InvoiceDeliveryMethods DelyMthd on saleInvc.DeliverMethod = DelyMthd.ID 
	left join cmn_PersonConnectedPersons PerConPerson on saleInvc.ConnectedPerson = PerConPerson.ID
	left join cmn_Persons ConPerson on ConPerson.ID = PerConPerson.ConnectedPerson
	left join afw_OptionSetItems ConPersonGender on ConPersonGender.ID = ConPerson.Gender
	inner join cmn_MeasurementUnits Units on InvcItm.MeasurementUnit = Units.ID 
	left join cmn_States States on States.ID = Person.State 
	left join cmn_Cities Cities on Cities.ID = Person.City 
	left join cmn_StuffDefs StuffDefs on stuffs.StuffDef = StuffDefs.ID and Units.ID = StuffDefs.MainMeasurementUnit  
	left join cmn_Services service on InvcItm.Service = service.ID and Units.ID = service.MeasurementUnit
	left join cmn_FinancialGroups Cofing on 
		(saleInvc.OrganizationUnit = Cofing.OrganizationUnit and 
		saleInvc.FinancialDocType = Cofing.FinancialDocType and 
		saleInvc.FinancialYear = Cofing.FinancialYear)
	left join (		
select	InvoiceItem.SalesInvoice, InvoiceItem.Stuff, InvoiceItem.Service, 
	case 
				when InvoiceItem.Stuff is not null then 
					case when isnull(PriceListStuffItem.UnitPrice, 0) = 0 then 0
						else (InvoiceItem.UnitPrice * InvoiceItem.Quantity) - ((isnull(PriceListStuffItem.UnitPrice, 0) * InvoiceItem.Quantity)) 
					end
				when InvoiceItem.Service is not null then 
					case when isnull(PriceListServiceItem.UnitPrice, 0) = 0 then 0
						else (InvoiceItem.UnitPrice * InvoiceItem.Quantity) - ((isnull(PriceListServiceItem.UnitPrice, 0) * InvoiceItem.Quantity)) 
					end			
			 end EkhtelafPrice
		from ps_SalesInvoiceItems InvoiceItem 
			left join cmn_Services Service on Service.ID = InvoiceItem.Service
			left join cmn_Stuffs Stuf on Stuf.ID = InvoiceItem.Stuff
			inner join cmn_StuffDefs StuffDef on StuffDef.ID = Stuf.StuffDef
			left outer join ps_PriceLists PriceList 
			left join ps_PriceListStuffItems PriceListStuffItem on PriceListStuffItem.PriceList = PriceList.ID
			left join ps_PriceListServiceItems PriceListServiceItem on PriceListServiceItem.PriceList = PriceList.ID			
				on 
				(PriceListStuffItem.StuffDef = StuffDef.ID or PriceListServiceItem.Service = Service.ID)
				and (InvoiceItem.UnitPrice &lt;&gt; PriceListStuffItem.UnitPrice or InvoiceItem.UnitPrice &lt;&gt; PriceListServiceItem.UnitPrice)								
				and PriceList.IsDefault = 1
	) EkhtelafPriceList on InvcItm.SalesInvoice = EkhtelafPriceList.SalesInvoice and
		(InvcItm.Stuff = EkhtelafPriceList.Stuff or InvcItm.Service = EkhtelafPriceList.Service)		
where saleInvc.ID = '{InvoiceID}'
order by  InvcItm.RowNumber</SqlCommand>
