        <Columns isList="true" count="12">
          <value>PersonID,System.Guid</value>
          <value>PersonName,System.String</value>
          <value>InvoiceType,System.String</value>
          <value>InvoiceType_PersianText,System.String</value>
          <value>TotalStuffAndServicesPrice,System.Decimal</value>
          <value>TotalTaxAndToll,System.Decimal</value>
          <value>GeneralDiscount,System.Int64</value>
          <value>StuffAndServicesTotalAmountAfterDiscount,System.Decimal</value>
          <value>FinalAmount,System.Decimal</value>
          <value>FinancialDocTypeID,System.Guid</value>
          <value>FinancialDocType_Text,System.String</value>
          <value>FinancialGroup_Text,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>SpringDS</Name>
        <NameInSource>SpringCon</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>select PersonID,
	PersonName,
	InvoiceType,
	InvoiceType_PersianText,
	sum(TotalStuffAndServicesPrice) TotalStuffAndServicesPrice,
	sum(TotalTaxAndToll) TotalTaxAndToll,
	sum(GeneralDiscount) GeneralDiscount,
	sum(StuffAndServicesTotalAmountAfterDiscount) StuffAndServicesTotalAmountAfterDiscount,
	sum(FinalAmount) FinalAmount,
	FinancialDocTypeID,
	FinancialDocType_Text
from (
select Person.ID PersonID,
		Person.FullName PersonName,
		TotalStuffAndServicesPrice,
		GeneralDiscount,
		StuffAndServicesTotalAmountAfterDiscount,
		TotalTaxAndToll,
		FinalAmount,
		IssueDate,
		SalesInvoice.FinancialYear,
		SalesInvoice.FinancialGroup,
		'Sales' InvoiceType,
		N'فروش' InvoiceType_PersianText,
		FinancialDocType.ID FinancialDocTypeID,
		FinancialDocType.Title FinancialDocType_Text
	from  cmn_PersonsLookUpView Person
		inner join ps_SalesInvoices SalesInvoice on Person.ID = SalesInvoice.Customer and SalesInvoice.IsProforma = 0
		left join cmn_FinancialGroups FinancialGroup on FinancialGroup.ID = SalesInvoice.FinancialGroup
		left join afw_OptionSetItems FinancialDocType on FinancialDocType.ID = FinancialGroup.FinancialDocType
	union
select Person.ID PersonID,
		Person.FullName PersonName,
		TotalStuffAndServicesPrice,
		GeneralDiscount,
		StuffAndServicesTotalAmountAfterDiscount,
		TotalTaxAndToll,
		FinalAmount,
		IssueDate,
		BuyInvoice.FinancialYear,
		BuyInvoice.FinancialGroup,
		'Buy' InvoiceType,
		N'خرید' InvoiceType_PersianText,
		FinancialDocType.ID FinancialDocTypeID,
		FinancialDocType.Title FinancialDocType_Text
	from  cmn_PersonsLookUpView Person
		inner join ps_BuyInvoices BuyInvoice on Person.ID = BuyInvoice.Seller
		left join cmn_FinancialGroups FinancialGroup on FinancialGroup.ID = BuyInvoice.FinancialGroup
		left join afw_OptionSetItems FinancialDocType on FinancialDocType.ID = FinancialGroup.FinancialDocType
) SubQuery
where cast(SubQuery.IssueDate as date) &gt;=  {FromDate} and cast(SubQuery.IssueDate as date) &lt;= {ToDate} and
		({FinancialYear} is null or SubQuery.FinancialYear = {FinancialYear}) and
		({Person} is null or SubQuery.PersonID = {Person}) and
		({InvoiceType} is null or SubQuery.InvoiceType = {InvoiceType}) and
		({FinancialDocType} is null or SubQuery.FinancialDocTypeID = {FinancialDocType})
group by PersonID,
	PersonName,
	InvoiceType,
	InvoiceType_PersianText,
	FinancialDocTypeID,
	FinancialDocType_Text</SqlCommand>
      </SpringDS>
